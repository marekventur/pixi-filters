{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": [
            "filters/led/src/",
            "filters/led/crt-lottes/"
        ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown",
        "./scripts/jsdoc-fix"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        },
        "applicationName": "PixiJS Filters by Marek Ventur",
        "footer": "Made by Marek Ventur (marekventur.com)",
        "copyright": "PixiJS Copyright Â© 2013-2017 Mat Groves.",
        "meta": {
            "title": "PixiJS Filters by Marek Ventur Documentation",
            "description": "API Documentation for PixiJS filters by Marek Ventur",
            "keyword": "filters, webgl, led"
        },
        "linenums": true
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    },
    "opts": {
        "encoding": "utf8",
        "recurse": true,
        "private": false,
        "lenient": true,
        "destination": "./docs",
        "template": "./node_modules/jaguarjs-jsdoc"
    }
}
